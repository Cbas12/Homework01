docker run -it \
  -e POSTGRES_USER= "root" \
  -e POSTGRES_PASSWORD= "root" \
  -e POSTGRES_DB= "ny_taxi" \
  -v C=/Users/Usuario/OneDrive/MiOneDrive/Cursos/Zoomcamp2025/postgres \
  -p 5432=5432 \
  postgres=13

